package hu.eltesoft.modelexecution.validation

import "http://www.eclipse.org/uml2/5.0.0/UML"

@ExactlyOne(message = "Two elements with the same qualified name", key = "elem")
pattern TwoElementWithSameName(qname, elem) {
	NamedElement.qualifiedName(elem, qname);
}

@ExactlyOne(message = "Redefinition of leaf element is not allowed", key = "redefining")
pattern LeafRedefined(elem : RedefinableElement, redefining) {
	RedefinableElement.redefinedElement(redefining, elem);
	RedefinableElement.isLeaf(elem, true);
}
